<h2>Electronics Page</h2>
<div class="button button-blue-back" type="button" id="load-button">Load Electronics</div>

<div class="electronics-content">

</div>

<script>
    document.querySelector("#load-button").addEventListener("click", async function() {
        var response =await fetch("electronics");
        var electronicsHtml = await response.text();
        document.querySelector(".electronics-content").innerHTML = electronicsHtml;
    });
</script>
