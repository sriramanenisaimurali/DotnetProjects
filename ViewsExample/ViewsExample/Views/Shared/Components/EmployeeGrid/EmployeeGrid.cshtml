<div class="box">
    <h3>@ViewBag.Grid.GridTitle</h3>
    <table class="table w-100">
        <thead>
            <tr>
                <th>
                    Employee Name
                </th>
                <th>
                    Employee Stream
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach(Employee employee in ViewBag.Grid.Employees)
            {
                <tr>
                    <td>@employee.Name</td>
                    <td>@employee.Domain</td>
                </tr>
            }
        </tbody>
    </table>
</div>